#!/bin/bash

shopt -s nullglob

for i in ~/.flud*/dht/*; do rm -f "$i"; done
for i in ~/.flud*/store/*; do rm -f "$i"; done
for i in ~/.flud*/meta/*; do rm -f "$i"; done
for i in ~/.flud*/dl/*; do rm -rf "$i"; done
for i in ~/.flud*/flud.log; do rm "$i"; done
for i in ~/.flud*/twistd.log*; do rm "$i"; done
